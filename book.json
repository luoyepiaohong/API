{
    "title": "API",
    "author": "mk",
    "description" : "Yunu API",
    "language": "zh-hans",
    "plugins": [
        "theme-api",
        "github",
        "book-summary-scroll-position-saver",
        "expandable-chapters",
        "splitter",
        "anchor-navigation-ex",
        "tbfed-pagefooter",
        "editlink",
        "styles-less",
        "collapsible-chapters",
        "versions"
    ],
    "theme-default": {
        "showLevel": true
    },
    "pluginsConfig": {
        "theme-api": {
            "split": true,
            "theme": "dark",
            "languages": [
                {
                    "lang": "HTML",
                    "name": "HTML",
                    "default": true
                }
            ]
        },
        "github": {
            "url": "https://github.com/mkzxcvbnm/API"
        },
        "tbfed-pagefooter": {
            "copyright":"yunu",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "editlink": {
            "base": "https://www.gitbook.com/book/mkzxcvbnm/api/edit",
            "label": ""
        },
        "versions": {
            "type": "branches"
        }
    }
}
